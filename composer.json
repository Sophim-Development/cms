{
    "name": "realwat/cms",
    "description": "A content management system developed by Sophim-Development",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^8.1",
        "composer/installers": "^2.0",
        "vlucas/phpdotenv": "^5.6",
        "phpmailer/phpmailer": "^6.9"
    },
    "autoload": {
        "psr-4": {
            "realwat\\cms\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    }
}
